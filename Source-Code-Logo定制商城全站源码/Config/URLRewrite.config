<?xml version="1.0"?>
<RewriterConfig>
  <Rules>
    <RewriterRule>
      <LookFor>admin</LookFor>
      <SendTo>manage</SendTo>
    </RewriterRule>
    <RewriterRule>
      <LookFor>~/admin/([\d\D]+).aspx</LookFor>
      <SendTo>~/manage/$1.aspx</SendTo>
    </RewriterRule>
    <RewriterRule>
      <LookFor>~/admin/([\d\D]+)/([\d\D]+).aspx</LookFor>
      <SendTo>~/manage/$1/$2.aspx</SendTo>
    </RewriterRule>
    <RewriterRule>
      <LookFor>~/admin/([\d\D]+)/([\d\D]+)/([\d\D]+).aspx</LookFor>
      <SendTo>~/manage/$1/$2/$3.aspx</SendTo>
    </RewriterRule>
  </Rules>
</RewriterConfig>